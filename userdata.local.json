{
  "settings": {
    "editor-font-size": "base",
    "editor-font-family": "monospace",
    "editor-show-line-numbers": true,
    "editor-syntax-highlighting": true,
    "editor-theme-highlights": "auto",
    "editor-spell-check": false,
    "editor-auto-save-enable": true,
    "editor-auto-save-delay": 1000,
    "fileSystem-backup-enable": false,
    "fileSystem-backup-auto-backup": false,
    "bibtexViewer-auto-tidy": false,
    "bibtexViewer-tidy-options": "standard",
    "imageViewer-auto-center": true,
    "imageViewer-quality": "high",
    "imageViewer-enable-panning": true,
    "imageViewer-enable-filters": true,
    "pdfViewer-auto-scale": true,
    "pdfViewer-rendering-quality": "high",
    "pdfRenderer-enable": true,
    "pdfRenderer-initial-zoom": "200",
    "pdfRenderer-text-selection": true,
    "pdfRenderer-annotations": true,
    "theme-plugin": "texlyre-wide-theme",
    "theme-variant": "dark",
    "typst-auto-compile-on-open": false,
    "latex-engine": "pdftex",
    "latex-texlive-endpoint": "http://localhost:5004",
    "latex-store-cache": true,
    "latex-store-working-directory": false,
    "fileSync-enable": true,
    "fileSync-auto-interval": 10,
    "fileSync-hold-timeout": 30,
    "fileSync-request-timeout": 60,
    "fileSync-conflict-resolution": "prefer-latest",
    "fileSync-server-url": "http://localhost:8080",
    "fileSync-notifications": true,
    "fileTree-filesystem-drag-drop": true,
    "fileTree-internal-drag-drop": true,
    "collab-signaling-servers": "ws://localhost:4444/",
    "collab-awareness-timeout": 30,
    "collab-auto-reconnect": false
  },
  "properties": {
    "sidebar-width:global": 502,
    "latex-output-width:global": 700,
    "sidebar-collapsed:global": false,
    "latex-output-collapsed:global": true,
    "log-visualizer-height:global": 600,
    "log-visualizer-collapsed:global": false,
    "pdf-renderer-zoom:global": 1,
    "pdf-renderer-scroll-view:global": false
  },
  "secrets": {}
}