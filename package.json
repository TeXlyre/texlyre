{
	"name": "texlyre",
	"private": true,
	"version": "0.3.4",
	"description": "A local-first LaTeX & Typst collaborative web editor ",
	"author": "Fares Abawi <fares@abawi.me> (https://abawi.me)",
	"license": "AGPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "https://github.com/texlyre/texlyre.git"
	},
	"type": "module",
	"scripts": {
		"start": "node scripts/pm.cjs run build:prod && node scripts/pm.cjs run test && node scripts/pm.cjs run preview",
		"predev": "node scripts/copy-pdf-cmaps.cjs && node scripts/copy-typst-assets.cjs",
		"dev": "node scripts/pm.cjs run generate-plugins && node scripts/pm.cjs vite",
		"dev:https": "node scripts/pm.cjs run generate-plugins && node scripts/pm.cjs cross-env VITE_USE_HTTPS=true vite --host",
		"build": "node scripts/pm.cjs tsc && node scripts/pm.cjs vite build",
		"build:prod": "node scripts/pm.cjs run generate-configs && node scripts/pm.cjs tsx scripts/update-sw-version.js && node scripts/pm.cjs run generate-plugins && node scripts/pm.cjs run generate-fonts && node scripts/pm.cjs tsc && node scripts/pm.cjs vite build",
		"build:local": "node scripts/pm.cjs tsx scripts/update-sw-version.js && node scripts/pm.cjs run generate-plugins && node scripts/pm.cjs run generate-fonts && node scripts/pm.cjs tsc && node scripts/pm.cjs vite build",
		"clean:cache": "node scripts/clean-cache.cjs",
		"generate-configs": "node scripts/pm.cjs tsx scripts/generate-configs.ts",
		"generate-plugins": "node scripts/pm.cjs tsx scripts/generate-plugins-index.js",
		"generate-fonts": "node scripts/generate-font-manifest.cjs",
		"lint": "node scripts/pm.cjs eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"clean": "node scripts/pm.cjs eslint . --ext ts,tsx --fix && node scripts/pm.cjs @biomejs/biome check --fix",
		"preview": "node scripts/pm.cjs vite preview",
		"test": "node scripts/pm.cjs jest",
		"test:watch": "node scripts/pm.cjs jest --watch",
		"test:coverage": "node scripts/pm.cjs jest --coverage",
		"test:unit": "node scripts/pm.cjs jest tests/unit",
		"test:integration": "node scripts/pm.cjs jest tests/integration",
		"test:components": "node scripts/pm.cjs jest tests/components",
		"test:services": "node scripts/pm.cjs jest tests/services",
		"test:ci": "node scripts/pm.cjs jest --ci --coverage --maxWorkers=2"
	},
	"dependencies": {
		"@biomejs/biome": "^1.8.3",
		"@codemirror/autocomplete": "^6.0.0",
		"@codemirror/commands": "^6.0.0",
		"@codemirror/lang-css": "^6.3.1",
		"@codemirror/lang-html": "^6.4.9",
		"@codemirror/lang-javascript": "^6.0.0",
		"@codemirror/lang-json": "^6.0.2",
		"@codemirror/lang-markdown": "=6.3.2",
		"@codemirror/lang-python": "^6.2.1",
		"@codemirror/language": "^6.0.0",
		"@codemirror/lint": "^6.0.0",
		"@codemirror/state": "^6.5.0",
		"@codemirror/search": "^6.5.0",
		"@codemirror/theme-one-dark": "^6.1.3",
		"@codemirror/view": "^6.0.0",
		"@lezer/common": "^1.0.0",
		"@lezer/generator": "^1.0.0",
		"@lezer/highlight": "^1.0.0",
		"@lezer/lr": "^1.0.0",
		"@myriaddreamin/typst-ts-renderer": "^0.6.1-rc3",
		"@myriaddreamin/typst-ts-web-compiler": "^0.6.1-rc3",
		"@myriaddreamin/typst.ts": "^0.6.1-rc2",
		"@picocss/pico": "^2.0.6",
		"@replit/codemirror-vim": "^6.3.0",
		"bib-editor": "^1.14.0",
		"codemirror": "^6.0.0",
		"codemirror-lang-bib": "^0.1.1",
		"codemirror-lang-latex": "^0.2.0",
		"codemirror-lang-typst": "^0.4.0",
		"file-saver": "^2.0.5",
		"filepizza-client": "^2.0.0-alpha.2",
		"idb": "^8.0.2",
		"jszip": "^3.10.1",
		"lib0": "^0.2.42",
		"lodash": "^4.17.21",
		"mime": "^4.0.1",
		"nanoid": "^5.0.5",
		"next": "^14.2.28",
		"pdfjs-dist": "5.3.31",
		"peerjs": "^1.5.1",
		"qrcode": "^1.5.4",
		"react": "^18.2.0",
		"react-ace": "^10.1.0",
		"react-dom": "^18.2.0",
		"react-pdf": "10.0.1",
		"vite-plugin-wasm": "^3.3.0",
		"workbox-build": "^7.3.0",
		"y-codemirror.next": "=0.3.5",
		"y-indexeddb": "^9.0.11",
		"y-protocols": "^1.0.6",
		"y-webrtc": "^10.2.0",
		"y-websocket": "^1.5.0",
		"yjs": "^13.5.51"
	},
	"overrides": {
		"rimraf": "^4.0.0"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.1.5",
		"@testing-library/react": "^14.1.2",
		"@testing-library/user-event": "^14.5.1",
		"@types/jest": "^29.5.11",
		"@types/node": "^20.10.6",
		"@types/qrcode": "^1.5.5",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@typescript-eslint/eslint-plugin": "^8.45.0",
		"@typescript-eslint/parser": "^8.45.0",
		"@vitejs/plugin-basic-ssl": "^2.0.0",
		"@vitejs/plugin-react": "^4.5.0",
		"cross-env": "^7.0.3",
		"eslint": "8.57.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"fake-indexeddb": "^5.0.2",
		"fs-extra": "^11.1.1",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"ts-jest": "^29.1.1",
		"ts-jest-mock-import-meta": "^1.3.1",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.6",
		"typescript": "^5.8.3",
		"typescript-eslint": "^7.3.1",
		"vite": "^6.3.5",
		"vite-plugin-pwa": "^0.21.1",
		"vite-plugin-static-copy": "^3.0.2",
		"workbox-window": "^7.0.0"
	}
}